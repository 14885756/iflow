{
  "name":"iflow",
  "version":"0.1",
  "author":"zhangsl",
  "desc":"版本控制与发布工具，基于git制作。输入help或h查看帮助",
  "branch":{
    "product":"master",
    "test":"sp-dev",
    "feature_prefix":"feature",
    "hotfix_prefix":"hotfix",
    "split":"/"
  },
  "sprint_format":"[0-9]{4}s[12]",
  "sprint_format_loose":"([0-9]{2}|[0-9]{4})s[12]",
  "alias":{
    "h":"help",
    "sp":"sprint",
    "ft":"feature",
    "fix":"hotfix",
    "c":"create",
    "t":"test",
    "p":"publish",
    "s":"show",
    "d":"delete",
    "br":"branch"
  },
  "cmd_cls":{
    "feature":"Feature",
    "hotfix":"Hotfix",
    "git":"Git",
    "rename":"Git",
    "help":"Extra",
    "cd":"Extra",
    "sprint":"Extra",
    "branch":"Extra",
    "h2f":"Extra",
    "h2h":"Extra",
    "f2h":"Extra",
    "f2f":"Extra",
    "pwd":"Extra",
    "version":"Extra",
    "alias":"Extra"
  }
}