{
  "name":"iflow",
  "version":"0.1",
  "author":"zhangsl",
  "desc":"版本控制与发布工具，基于git制作。输入help或h查看帮助",
  "branch":{
    "product":"master",
    "test":"sp-dev",
    "feature_prefix":"feature",
    "hotfix_prefix":"hotfix",
    "split":"/"
  },
  "sql_dir":"db/{sprint}/",
  "sql_file_suffix":".sql",
  "sprint_format":"[0-9]{4}s[12]",
  "sprint_format_loose":"([0-9]{2}|[0-9]{4})s[12]",
  "alias":{
    "h":"help",
    "sp":"sprint",
    "ft":"feature",
    "fix":"hotfix",
    "c":"create",
    "t":"test",
    "p":"product",
    "s":"show",
    "d":"delete",
    "cmt":"commit",
    "ck":"checkout"
  },
  "cmd_cls":{
    "feature":"Develop",
    "hotfix":"Develop",
    "git":"Git",
    "rename":"Git",
    "help":"Extra",
    "cd":"Extra",
    "sprint":"Extra",
    "pwd":"Extra",
    "version":"Extra",
    "alias":"Extra",
    "sql":"Extra",
    "exit":"Extra",
    "h2f":"Transform",
    "f2h":"Transform",
    "f2f":"Transform",
    "commit":"Git"
  }
}