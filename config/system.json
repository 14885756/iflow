    "feature":"feature",
    "hotfix":"hotfix",
    "create":"create",
    "test":"test",
    "publish":"publish",
    "show":"show",
    "delete":"delete",
    "branch":"branch",
    "cd":"cd",
    "git":"git",
    "help":"help",
    "h":"help",
    "sp":"sprint",
    "ft":"feature",
    "fix":"hotfix",
    "c":"create",
    "t":"test",
    "p":"publish",
    "s":"show",
    "d":"delete",
    "br":"branch"
  },
  "cmd_cls":{
    "feature":"Feature",
    "hotfix":"Hotfix",
    "help":"Help",
    "cd":"Extra",
    "git":"Extra",
    "sprint":"Extra",
    "branch":"Extra",
    "h2f":"Extra",
    "h2h":"Extra",
    "f2h":"Extra",
    "f2f":"Extra",
    "rename":"Extra"
  }
